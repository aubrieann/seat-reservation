.gcfoot
    display inline-block
    margin 2% auto

nav
    width 100%
    background-color #008BCE

ul.gcnav
    display inline-block
    list-style-type none
    background-color #008BCE
    height 80px

li.gcnav 
    float left
    font-size 12px
    font-family Arial
    height 80px

li.gcnav a
    display block
    color white
    text-align center
    padding 14px 14px
    text-decoration none
    font-size 125%

li.gcnav ahover
    background-color #F58235
    color white
    text-decoration none

#gcnav a
	display block
	text-align center
	padding 14ps 16px
#chair
	width 10px
	height 210px

li.gcfoot
    list-style-type none
    text-align right
#holder
	height 200px
	width 500px
	background-color #f5f5f5
	border 3px solid #a4a4a4
	margin 0px 0px 0px 15px

.place
	position relative
	margin 7px

.place a
	font-size 0.6em

.place li
	list-style none outside none
	position absolute

.place lihover
	background-color red

.place .seat
	backgroundurl("images/available_seat_img.gif") no-repeat scroll 0 0 transparent
	height 30px
	width 30px
	display block

.place .pickedSeat
	background-image url("images/booked_seat_img.gif")

.place .pickingSeat 
	background-image url("images/selected_seat_img.gif")

.place .row-3, .place .row-4
	margin-top 10px

.seatDescription li
	vertical-align middle
	list-style none outside none
	padding-left 5px
	height 35px
	float left

